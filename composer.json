{
    "name": "narendravaghela/cakephp-remember-me",
    "description": "RememberMe plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "remember", "me"],
    "homepage": "https://github.com/narendravaghela/cakephp-remember-me",
    "authors": [
        {
            "name": "Narendra Vaghela",
            "homepage": "http://www.narendravaghela.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": ">=3.1.0 <4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "RememberMe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RememberMe\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
